{
  "name": "react-js-rollup-library-docs",
  "version": "0.3.10-alpha.2",
  "description": "> TODO: description",
  "private": true,
  "author": "Arpit Malik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitmalik832/react-ts-monorepo"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install dependencies')\"",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "rm-yarn-lock": "rimraf yarn.lock",
    "debug": "node --inspect",
    "debug-brk": "node --inspect-brk",
    "clean": "yarn rm-yarn-lock && yarn rm-node_modules",
    "docusaurus": "docusaurus",
    "start": "docusaurus start --config docusaurus.config.staging.ts",
    "start:beta": "docusaurus start --config docusaurus.config.beta.ts",
    "start:prod": "docusaurus start --config docusaurus.config.prod.ts",
    "build": "yarn rm-build && docusaurus build --config docusaurus.config.staging.ts",
    "build:beta": "yarn rm-build && docusaurus build --config docusaurus.config.beta.ts",
    "build:prod": "yarn rm-build && docusaurus build --config docusaurus.config.prod.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --config docusaurus.config.staging.ts",
    "serve:beta": "docusaurus serve --config docusaurus.config.beta.ts",
    "serve:prod": "docusaurus serve --config docusaurus.config.prod.ts",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "create-version": "yarn docusaurus docs:version",
    "lint-tsc": "tsc --noEmit",
    "lint-js": "eslint --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint",
    "prettier": "prettier --check",
    "lint": "yarn lint-js \"**/*.{mts,cts,ts,tsx,mjs,cjs,js,jsx,mdx}\" && yarn lint-css \"**/*.{css,scss}\" && yarn prettier .",
    "lint-js:fix": "yarn lint-js --fix",
    "lint-css:fix": "yarn lint-css --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "yarn lint-js:fix \"**/*.{mts,cts,ts,tsx,mjs,cjs,js,jsx,mdx}\" && yarn lint-css:fix \"**/*.{css,scss}\" && yarn prettier:fix .",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:clearCache": "jest --clearCache",
    "test:watch:quick": "yarn test:watch --maxWorkers=50%",
    "test:watch:inBand": "yarn test:watch --runInBand",
    "test:coverage:silent": "yarn test:coverage --silent",
    "test:coverage:quick": "yarn test:coverage --maxWorkers=50%",
    "test:coverage:inBand": "yarn test:coverage --runInBand",
    "test:coverage:silent:quick": "yarn test:coverage:silent --maxWorkers=50%",
    "test:coverage:silent:inBand": "yarn test:coverage:silent --runInBand",
    "knip": "knip",
    "knip:debug": "yarn knip --debug",
    "knip:check-files": "yarn knip --include files",
    "knip:w/o-devDependencies": "yarn knip --exclude devDependencies",
    "knip:prod": "yarn knip --production",
    "knip:prod:check-files": "yarn knip:prod --include files"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-import-attributes": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@testing-library/jest-dom": "^6.6.1",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.33.2",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "stylelint": "^16.10.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@docusaurus/theme-classic": "^3.5.2",
    "@mdx-js/react": "^3.0.1",
    "@svgr/webpack": "^8.1.0",
    "clsx": "^2.1.1",
    "docusaurus-theme-search-typesense": "^0.22.0",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "url-loader": "^4.1.1"
  },
  "resolutions": {
    "path-to-regexp": "^3.3.0"
  }
}
